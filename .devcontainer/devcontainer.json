{
  "name": "latex",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "vscode",
  "runServices": [
    "vscode"
  ],
  "shutdownAction": "stopCompose",
  "workspaceFolder": "/workspace",
  "postCreateCommand": "",
  "customizations": {
    "vscode": {
      "extensions": [
        "james-yu.latex-workshop",
        "eamodio.gitlens",
        "shardulm94.trailing-spaces",
        "stkb.rewrap",
        "hediet.vscode-drawio",
        "tintinweb.graphviz-interactive-preview",
        "jebbs.plantuml",
        "ms-python.python",
        "taichi.vscode-textlint",
        "ms-python.autopep8"
      ]
    }
  }
}